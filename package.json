{
  "name": "najs-eloquent-example",
  "version": "0.2.2",
  "scripts": {
    "start":
      "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "prepublishOnly": "tsc -d -p ./ --outDir dist",
    "watch-ts": "tsc -w",
    "serve": "nodemon ./dist"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "collect.js": "^4.0.2",
    "concurrently": "^3.5.1",
    "config": "^1.29.2",
    "mongoose": "^4.7.31",
    "najs": "^0.1.6",
    "najs-eloquent": "^0.2.9",
    "sinon": "^4.1.3"
  },
  "devDependencies": {
    "@types/config": "^0.0.33",
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.91",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^4.7.31",
    "@types/pluralize": "^0.0.28",
    "@types/sinon": "^4.1.2",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "nodemon": "^1.14.11",
    "power-assert": "^1.4.4",
    "ts-jest": "^21.2.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
